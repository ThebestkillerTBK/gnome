{
  "schemaVersion": 1,
  "id": "gnome-addon",
  "version": "${version}",
  "name": "Gnome",
  "description": "A standalone version for GreemDev's meteor fork.",
  "authors": [
    "SH_Sodawater"
  ],
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "me.soda.gnome.Addon"
    ]
  },
  "mixins": [
    "gnome.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=${mc_version}",
    "meteor-client": "*"
  }
}
